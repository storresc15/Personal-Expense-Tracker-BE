{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", "tab"],
        "quotes": ["warn", "single"],
        "semi": ["warn", "always"]
    }
}
